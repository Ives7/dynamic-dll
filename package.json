{
  "name": "@shuvi/dynamic-dll",
  "version": "0.0.1-pre.1",
  "license": "MIT",
  "homepage": "https://github.com/shuvijs/dynamic-dll",
  "repository": {
    "type": "git",
    "url": "git@github.com:shuvijs/dynamic-dll.git"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "esm",
    "lib"
  ],
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "dev": "run-p watch:*",
    "watch:esm": "tsc -p tsconfig.build.esm.json -w",
    "watch:cjs": "tsc -p tsconfig.build.cjs.json -w",
    "prebuild": "rimraf lib esm",
    "build": "run-p build:*",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:cjs": "tsc -p tsconfig.build.cjs.json"
  },
  "dependencies": {
    "@umijs/bundler-utils": "4.0.0-rc.1",
    "@umijs/utils": "4.0.0-rc.1",
    "mrmime": "1.0.0",
    "webpack-virtual-modules": "0.4.3"
  },
  "devDependencies": {
    "webpack": "5.70.0",
    "webpack-chain": "6.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "webpack": "^5.70.0"
  }
}
